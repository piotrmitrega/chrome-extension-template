{
  "action": {
    "default_icon": "public/dev-icon-32.png",
    "default_popup": "src/roots/popup/index.html"
  },
  "icons": {
    "128": "public/dev-icon-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "identity",
    "storage",
    "scripting"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "icon-128.png",
        "icon-32.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
